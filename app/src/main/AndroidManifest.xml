<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mycaptain.chatup">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Fragments.message"></activity>
        <activity android:name=".Fragments.fragmentusers" />
        <activity android:name=".Fragments.fragmentprofile" />
        <activity android:name=".Fragments.fragmentchats" />
        <activity android:name=".Fragments.ResetPasswordActivity" />
        <activity android:name=".Fragments.Register" />
        <activity android:name=".Fragments.LoginActivity" />
        <activity android:name=".Fragments.Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Fragments.MainActivity" />
    </application>

</manifest>